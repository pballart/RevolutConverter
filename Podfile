target 'RevolutConverter' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RevolutConverter
  pod 'Moya'
  pod 'Result'

  target 'RevolutConverterTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Moya'
    pod 'Result'
  end

end
